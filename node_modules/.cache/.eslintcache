\")[0] * 1;\n        const date = new Date();\n        const now = date.getHours();\n        if (now >= opening && now <= closing) return true;\n        else return false;\n      default:\n        return true;\n    }\n  };\n  return (\n    <div className=\"restaurants-grid\">\n      {restaurants.map((item: any) => {\n        if (passFilter(item))\n          return (\n            <Link to={`/restaurant/${item.name}`} className=\"to-restaurant-btn\">\n              <BeigeCard\n                key={item.name}\n                title={item.name}\n                detail={item.chef}\n                img={item.mobileImage}\n              ></BeigeCard>\n            </Link>\n          );\n      })}\n    </div>\n  );\n};\nexport default RestaurantsGrid;\n","/Users/yardentsaraf/Desktop/EpicureApp/src/components/header/Header.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/pages/allChefsPage/ChefsGrid.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/pages/homePage/Icons.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/layout/card/Card.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/layout/search/Search.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/layout/card/DishSmallCard.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/layout/card/ChefCard.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/layout/button/NavLinkTemplate.tsx",[],[],"/Users/yardentsaraf/Desktop/EpicureApp/src/components/layout/form/Input.tsx",["249"],[],"import React, { Fragment, useState } from 'react';\nimport './Input.scss';\n\nconst Input: React.FC<{content: string, type:string, placeholder:string}> = (props) => {\n    const [enteredInput, setEnteredInput]= useState('');\n    const [notRadioOrCheckbox, setNotRadioOrCheckbox] = useState(props.type !== 'radio' && props.type !== 'checkbox');\n    const changeInputHandler= (event:React.FormEvent<HTMLInputElement>) => {\n        event.preventDe